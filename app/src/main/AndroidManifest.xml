<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myquizapp">

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.myquizapp.ScoreActivity" />
        <activity android:name="com.example.myquizapp.QuestionActivity" />
        <activity android:name="com.example.myquizapp.SetsActivity" />
        <activity android:name="com.example.myquizapp.CategoryActivity" />
        <activity android:name="com.example.myquizapp.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myquizapp.MainActivity">
        </activity>
    </application>

</manifest>